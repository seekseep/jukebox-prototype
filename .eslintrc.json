{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "semi": ["error", "never"],
    "key-spacing": ["error", { "align": "colon" }],
    "jsx-quotes": ["error", "prefer-double"],
    "quotes": ["error", "single"],
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": "error",
    "no-unused-vars": ["error"]
  }
}
